<?xml version="1.0" encoding="UTF-8"?>
<mime-info>
  <mime-type type="application/ead+xml">
    <root-XML localName="ead"/>
    <sub-class-of type="application/xml"/>
    <glob pattern="*.ead.xml"/>
   </mime-type>
   <mime-type type="application/eag+xml">
     <sub-class-of type="application/xml"/>
     <root-XML localName="eag"/>
     <sub-class-of type="application/xml"/>
     <glob pattern="*.eag.xml"/>
   </mime-type>
   <mime-type type="application/eac+xml">
     <root-XML localName="eac"/>
     <sub-class-of type="application/xml"/>
     <glob pattern="*.eac.xml"/>
   </mime-type>   
   <mime-type type="application/tei+xml">
     <root-XML localName="TEI" namespaceURI="http://www.tei-c.org/ns/1.0"/>
     <sub-class-of type="application/xml"/>
     <acronym>TEI/XML</acronym>
     <glob pattern="*.tei"/>
   </mime-type>   
   <mime-type type="application/oai-pmh+xml">
     <root-XML localName="OAI-PMH" namespaceURI="http://www.openarchives.org/OAI/2.0/"/>
     <sub-class-of type="application/xml"/>
     <acronym>OAI-PMH/XML</acronym>
     <glob pattern="*.oai"/>
   </mime-type>   
  <mime-type type="application/json">
     <acronym>JSON</acronym>
     <glob pattern="*.json"/>
  </mime-type>
</mime-info>
